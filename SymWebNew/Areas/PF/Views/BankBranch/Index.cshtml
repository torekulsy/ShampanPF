<div class="headrow">
    <button type="button" title="Add New Bank Branch" data-url="/@ViewBag.TransType/BankBranch/Create" onclick="GoTo(this)" id="" class=" sym-btn-add ">&nbsp;New</button>
    <button type="button" title="Delete Bank Branch" data-url="/@ViewBag.TransType/BankBranch/Delete" onclick="deletedData(this, 'myDataTable');" id="" class="sym-btn-remove">&nbsp;Delete</button>
    <label class="pull-right">Bank Branch </label>
</div>
<div class="thinbox">
    <div class="th_thinbox">
        <p>Bank Branch List</p>
    </div>
    <table id="myDataTable" class="display">
        <thead>
            <tr>
                <th class="center">
                    <input type="checkbox" class="chkAll" />Select All</th>
                <th class="center">Bank Name</th>
                <th class="center">Branch Name</th>
                <th class="center">Branch Address </th>
                <th class="center">Bank Account Type</th>
                <th class="center">Bank Account No</th>
                


@*  
BankName      
BranchName
BranchAddress 
BankAccountType
BankAccountNo  
*@
            </tr>
        </thead>
        <tbody>
        </tbody>
    </table>
    <br />
</div>
<div class="headrow">
    <button type="button" title="Add New Bank Branch" data-url="/@ViewBag.TransType/BankBranch/Create" onclick="GoTo(this)" id="" class=" sym-btn-add ">&nbsp;New</button>
    <button type="button" title="Delete Bank Branch" data-url="/@ViewBag.TransType/BankBranch/Delete" onclick="deletedData(this, 'myDataTable');" id="" class="sym-btn-remove">&nbsp;Delete</button>
</div>
<script language="javascript" type="text/javascript">
    $(document).ready(function () {
        SelectAllForDelete();
        $.datepicker.regional[""].dateFormat = 'dd/M/y';
        $.datepicker.setDefaults($.datepicker.regional['']);
        var oTable = $('#myDataTable').dataTable({
            "bServerSide": true,
            "sAjaxSource": "/@ViewBag.TransType/BankBranch/_index",
            "bProcessing": true,
            "aLengthMenu": tablelength(),
            "bJqueryUI": true,
            "bAutoWidth": false,
            "aoColumns": [
                            {
                                "sName": "Id",
                                "bSearchable": false,
                                "bSortable": false,
                                "sWidth": "12%",
                                "sClass": "center",
                                "fnRender": function (oObj) {
                                    return ' <input onclick="CheckAll(this)" type="checkbox" data-Id="' + oObj.aData[0] + '">'
                                        + '&nbsp; <a id=""  title="Click on for Edit"  style="text-decoration:none;"   href=\"/@ViewBag.TransType/BankBranch/Edit/' + oObj.aData[0] + '\" class=" innerListLink sym-btn-edit-row"></a>'
                                        + '&nbsp; <a title="Click on for Delete"  data-url="/@ViewBag.TransType/BankBranch/Delete" onclick="deletedData(this, myDataTable,\'' + oObj.aData[0] + '\');"  class="sym-btn-remove-row"></a>'
                                    ;
                                }
                            }
                            , { "sName": "BankName", "sWidth": "10%" }
                            , { "sName": "BranchName", "sWidth": "10%" }
                            , { "sName": "BranchAddress", "sWidth": "10%" }
                            , { "sName": "BankAccountType", "sWidth": "10%" }
                            , { "sName": "BankAccountNo", "sWidth": "10%" }
                           

                            

            ]
        });
    });
</script>
