<div class="headrow">
    <button type="button" title="Add New Settlement Policy" data-url="/@ViewBag.TransType/AutoJournalSetup/Create" onclick="GoTo(this)" id="" class=" sym-btn-add ">&nbsp;New</button>
    <button type="button" title="Delete Settlement Policy" data-url="/@ViewBag.TransType/AutoJournalSetup/Delete" onclick="deletedData(this, 'myDataTable');" id="" class="sym-btn-remove">&nbsp;Delete</button>
    <label class="pull-right">Auto Journal Setup</label>
</div>
<div class="thinbox">
    <div class="th_thinbox">
        <p>Auto Journal Setup</p>
    </div>
    <table id="myDataTable" class="display">
        <thead>
            <tr>
                <th class="center">
                    <input type="checkbox" class="chkAll" />Select All</th>
                <th class="center">Journal For</th>
                <th class="center">Journal Name</th>
                <th class="center">Nature</th>
                <th class="center">COAID</th>
                <th class="center">Active/InActive</th>
            </tr>
        </thead>
        <tbody>
        </tbody>
    </table>
    <br />
</div>
<div class="headrow">
    <button type="button" title="Add New Auto Journal Setup" data-url="/@ViewBag.TransType/AutoJournalSetup/Create" onclick="GoTo(this)" id="" class=" sym-btn-add ">&nbsp;New</button>
    <button type="button" title="Delete Auto Journal Setup" data-url="/@ViewBag.TransType/AutoJournalSetup/Delete" onclick="deletedData(this, 'myDataTable');" id="" class="sym-btn-remove">&nbsp;Delete</button>
</div>
<script language="javascript" type="text/javascript">
    $(document).ready(function () {
        SelectAllForDelete();
        $.datepicker.regional[""].dateFormat = 'dd/M/y';
        $.datepicker.setDefaults($.datepicker.regional['']);
        var oTable = $('#myDataTable').dataTable({
            "bServerSide": true,
            "sAjaxSource": "/@ViewBag.TransType/AutoJournalSetup/_index",
            "bProcessing": true,
            "aLengthMenu": tablelength(),
            "bJqueryUI": true,
            "bAutoWidth": false,
            "aoColumns": [
                            {
                                "sName": "Id",
                                "bSearchable": false,
                                "bSortable": false,
                                "sWidth": "12%",
                                "sClass": "center",
                                "fnRender": function (oObj) {
                                    return ' <input onclick="CheckAll(this)" type="checkbox" data-Id="' + oObj.aData[0] + '">'
                                        + '&nbsp; <a id="lnkEdit1"  title="Click on for Edit" style="text-decoration:none;"    href=\"/@ViewBag.TransType/AutoJournalSetup/Edit/' + oObj.aData[0] + '\" class=" innerListLink sym-btn-edit-row"></a>'
                                        + '&nbsp; <a title="Click on for Delete" style="text-decoration:none;" data-url="/@ViewBag.TransType/AutoJournalSetup/Delete" onclick="deletedData(this, myDataTable,\'' + oObj.aData[0] + '\');"  class="sym-btn-remove-row"></a>'
                                    ;
                                }
                            }
                            , { "sName": "PolicyName", "sWidth": "10%" }
                            , { "sName": "JobAgeInMonth", "sWidth": "10%" }
                            , { "sName": "EmployeeContributionRatio ", "sWidth": "10%" }
                            , { "sName": "EmployeerContributionRatio", "sWidth": "10%" }
                            , { "sName": "Remarks", "sWidth": "10%" }
                           

                            

            ]
        });
    });
</script>
