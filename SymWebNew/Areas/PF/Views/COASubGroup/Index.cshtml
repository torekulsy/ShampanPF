<div class="headrow">
    <div class="pull-left">
        <button type="button" title="Add New COAGroup" data-url="/@ViewBag.TransType/COASubGroup/Create" onclick="GoTo(this)" id="" class=" sym-btn-add ">&nbsp;New</button>
    </div>
</div>
<script language="javascript" type="text/javascript">
    $(document).ready(function () {
        SelectAllForDelete();
        $.datepicker.regional[""].dateFormat = 'dd/M/y';
        $.datepicker.setDefaults($.datepicker.regional['']);
        var oTable = $('#myDataTable').dataTable({
            "bServerSide": true,
            "sAjaxSource": "/@ViewBag.TransType/COASubGroup/_index",
            "bProcessing": true,
            "aLengthMenu": tablelength(),
            "bJqueryUI": true,
            "bAutoWidth": false,
            "aoColumns": [
                            {
                                "sName": "Id",
                                "bSearchable": false,
                                "bSortable": false,
                                "sWidth": "10%",
                                "sClass": "center",
                                "fnRender": function (oObj) {

                                    return '  <a id=""  title="Click on for Edit" style="text-decoration:none;"    href=\"/@ViewBag.TransType/COASubGroup/Edit/' + oObj.aData[0] + '\" class="sym-btn-edit-row"></a>' + ' ';
                                }
                            }
                            , { "sGroupName": "GroupName ", "sWidth": "15%" }
                            , { "sGroupName": "Name ", "sWidth": "15%" }
                            , { "sIsActive": "IsActive", "sWidth": "10%" }
                            , { "sRemarks": "Remarks", "sWidth": "10%" }
            ]
        });
    });
</script>

