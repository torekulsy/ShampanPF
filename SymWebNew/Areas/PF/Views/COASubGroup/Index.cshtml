    <label class="pull-right">COA Sub Group </label>
</div>
<div class="thinbox">
    <div class="th_thinbox">
        <p>COA Sub Group List</p>
    </div>
    <p>
    </p>
    <table id="myDataTable" class="display">
        <thead>
            <tr>
                <th class="center">Action</th>
                <th class="center">COA Group</th>
                <th class="center">Sub Group Name</th>
                <th class="center">Active/Inactive</th>
                <th class="center">Remarks</th>
            </tr>
        </thead>
        <tbody></tbody>
    </table>
