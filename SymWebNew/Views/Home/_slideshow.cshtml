@{
    string mySettingValue = System.Configuration.ConfigurationManager.AppSettings["Company"];
}

<div class="carousel slide article-slide" id="article-photo-carousel">
    <div style="text-align: center; font-weight: bold;font-size: 1.5em">
        @*@mySettingValue<br>*@
        @*Human Resource Management System*@
    </div>
    <!-- Wrapper for slides -->
    <div class="carousel-inner cont-slider">

        <div class="item active">
            <img src="~/Images/symphonyLogo1.png" style="width: 810px; height: 400px;" />
        </div>
        <div class="item">

            <img src="~/Images/sapamahrm_banner.png" style="width:810px;height:400px" />
        </div>
        <div class="item">
            <img src="~/Images/payroll-1.jpg" style="width:810px;height:400px" />
        </div>
        <div class="item">
            <img src="~/Images/payroll-4.jpg" style="width:810px;height:400px" />
        </div>
    </div>

    <!-- Indicators -->
    <ol class="carousel-indicators">
        <li class="active" data-slide-to="0" data-target="#article-photo-carousel">
            <img alt="" src="~/Images/symphonyLogo1.png" width="180" height="250">
        </li>
        <li class="" data-slide-to="1" data-target="#article-photo-carousel">
            <img alt="" src="~/Images/sapamahrm_banner.png" width="180" height="250">
        </li>
        <li class="" data-slide-to="2" data-target="#article-photo-carousel">
            <img alt="" src="~/Images/payroll-1.jpg" width="180" height="250">
        </li>
        <li class="" data-slide-to="3" data-target="#article-photo-carousel">
            <img alt="" src="~/Images/payroll-4.jpg" width="180" height="250">
        </li>
    </ol>
</div>
<script>
    $(function () {
    $('.carousel').carousel({
        interval: false
    })
    })
</script>